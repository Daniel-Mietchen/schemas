# Shape Expression for Diseases in Wikidata 
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX prv: <http://www.wikidata.org/prop/reference/value/>
PREFIX pr:  <http://www.wikidata.org/prop/reference/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <http://schema.org/> 
PREFIX do: <http://purl.obolibrary.org/obo/DOID_>
PREFIX doio: <http://identifiers.org/doid/>
PREFIX mir: <http://www.ebi.ac.uk/miriam/main/collections/>

PREFIX disease_ontology: <https://raw.githubusercontent.com/shexSpec/schemas/master/Wikidata/genewiki/diseases/>
IMPORT disease_ontology:wikidata-disease-statements.shex


start = @<wikidata-disease>

<wikidata-disease> {
	# Descriptors
	rdfs:label LITERAL+ ;
	schema:description LITERAL* ;

	# Statements 
	p:P31	@disease_ontology:P31_disease-class ;
	p:P279  @disease_ontology:P279_disease-parent-class* ;
	p:P2888 @disease_ontology:P2888_disease-ontology-iri OR @disease_ontology:P2888_identifiers-org-iri ;

	## IDENTIFIERS
    p:P699 @disease_ontology:P699_disease_ontology_id ;
    p:P486 @disease_ontology:P486_mesh_id* ;
    p:P492 @disease_ontology:P492_omim_id* ;
    p:P493 @disease_ontology:P493_icd_9* ;
    p:P494 @disease_ontology:P494_icd_10* ;
    p:P1550 @disease_ontology:P1550_orphanet_id* ;
	p:P1748 @disease_ontology:P1748_nci_thesaurus* ;
    p:P2892 @disease_ontology:P2892_umls-cui* ;
}